{
  "name": "inventario-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npm run lint:js & npm run lint:styles & wait",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx",
    "lint:js:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "lint:styles": "stylelint \"src/**/*.{scss,vue}\"",
    "lint:styles:fix": "stylelint \"src/**/*.{scss,vue}\" --fix",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "7.0.0",
    "@fortawesome/free-brands-svg-icons": "7.0.0",
    "@fortawesome/free-regular-svg-icons": "7.0.0",
    "@fortawesome/free-solid-svg-icons": "7.0.0",
    "@fortawesome/vue-fontawesome": "3.1.1",
    "@primeuix/themes": "1.2.3",
    "axios": "1.11.0",
    "pdfjs-dist": "5.4.54",
    "pinia": "3.0.3",
    "primeicons": "7.0.0",
    "primevue": "4.3.7",
    "vue": "3.5.18",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.33.0",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-vue": "10.4.0",
    "globals": "16.3.0",
    "jsdom": "26.1.0",
    "postcss-html": "1.8.0",
    "postcss-scss": "4.0.9",
    "sass": "1.90.0",
    "stylelint": "16.23.1",
    "stylelint-config-standard-scss": "15.0.1",
    "stylelint-scss": "6.12.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.1",
    "vite": "7.1.2",
    "vitest": "3.2.4",
    "vue-eslint-parser": "10.2.0"
  },
  "engines": {
    "node": "22.18.0"
