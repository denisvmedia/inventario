{
  "name": "inventario-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npm run lint:js & npm run lint:styles & wait",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx",
    "lint:js:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "lint:styles": "stylelint \"src/**/*.{scss,vue}\"",
    "lint:styles:fix": "stylelint \"src/**/*.{scss,vue}\" --fix",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-brands-svg-icons": "7.1.0",
    "@fortawesome/free-regular-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/vue-fontawesome": "3.1.2",
    "@primeuix/themes": "1.2.5",
    "axios": "1.13.2",
    "pdfjs-dist": "5.4.449",
    "pinia": "3.0.4",
    "primeicons": "7.0.0",
    "primevue": "4.4.1",
    "vue": "3.5.25",
    "vue-router": "4.6.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.37.0",
    "@typescript-eslint/eslint-plugin": "8.50.0",
    "@typescript-eslint/parser": "8.50.0",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitest/coverage-v8": "4.0.9",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-vue": "10.5.1",
    "globals": "16.5.0",
    "jsdom": "27.2.0",
    "postcss-html": "1.8.0",
    "postcss-scss": "4.0.9",
    "sass": "1.94.2",
    "stylelint": "16.25.0",
    "stylelint-config-standard-scss": "16.0.0",
    "stylelint-scss": "6.12.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.47.0",
    "vite": "7.3.0",
    "vitest": "4.0.16",
    "vue-eslint-parser": "10.2.0"
  },
  "engines": {
    "node": "22.20.0"
  },
  "volta": {
    "node": "22.20.0",
    "npm": "11.6.4"
  }
